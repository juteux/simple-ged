<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
						"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.simple.ged.connector.plugins.SimpleGedPluginProperty" table="plugin_property" lazy="false" >

		<id name="id" column="rowid">
			<generator class="native"></generator>
		</id>
		
		<property name="propertyKey" column="name"></property>
		<property name="propertyValue" column="value"></property>
		<property name="hidden" column="hidden"></property>

	</class>

</hibernate-mapping>

